        <servlet-name>VerificarReservaUsuarioServlet</servlet-name>
        <url-pattern>/VerificarReservaUsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrestamoUsuarioServlet</servlet-name>
        <url-pattern>/PrestamoUsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUbicacionServlet</servlet-name>
        <url-pattern>/GetUbicacionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MobilWebService</servlet-name>
        <url-pattern>/MobilWebService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FechaDevolucionServlet</servlet-name>
        <url-pattern>/FechaDevolucionServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
