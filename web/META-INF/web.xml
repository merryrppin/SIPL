<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <listener>
        <listener-class>com.sun.xml.ws.transport.http.servlet.WSServletContextListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>MaterialServlet</servlet-name>
        <servlet-class>sipl.servlets.MaterialServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioServlet</servlet-name>
        <servlet-class>sipl.servlets.UsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MaterialesPrestamoServlet</servlet-name>
        <servlet-class>sipl.servlets.MaterialesPrestamoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FechaPrestamoServlet</servlet-name>
        <servlet-class>sipl.servlets.FechaPrestamoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MultaUsuarioServlet</servlet-name>
        <servlet-class>sipl.servlets.MultaUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReservaUsuarioServlet</servlet-name>
        <servlet-class>sipl.servlets.ReservaUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUploadServlet</servlet-name>
        <servlet-class>sipl.servlets.FileUploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MaterialesReservaServlet</servlet-name>
        <servlet-class>sipl.servlets.MaterialesReservaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerificarReservaUsuarioServlet</servlet-name>
        <servlet-class>sipl.servlets.VerificarReservaUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrestamoUsuarioServlet</servlet-name>
        <servlet-class>sipl.servlets.PrestamoUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetUbicacionServlet</servlet-name>
        <servlet-class>sipl.servlets.GetUbicacionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MobilWebService</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>FechaDevolucionServlet</servlet-name>
        <servlet-class>sipl.servlets.FechaDevolucionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MaterialServlet</servlet-name>
        <url-pattern>/MaterialServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServlet</servlet-name>
        <url-pattern>/UsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MaterialesPrestamoServlet</servlet-name>
        <url-pattern>/MaterialesPrestamoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FechaPrestamoServlet</servlet-name>
        <url-pattern>/FechaPrestamoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MultaUsuarioServlet</servlet-name>
        <url-pattern>/MultaUsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReservaUsuarioServlet</servlet-name>
        <url-pattern>/ReservaUsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUploadServlet</servlet-name>
        <url-pattern>/FileUploadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MaterialesReservaServlet</servlet-name>
        <url-pattern>/MaterialesReservaServlet</url-pattern>
    </servlet-mapping>
